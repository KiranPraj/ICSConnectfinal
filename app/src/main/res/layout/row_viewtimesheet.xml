<?xml version="1.0" encoding="utf-8"?>
<TableRow
        xmlns:android="http://schemas.android.com/apk/res/android"
              xmlns:app="http://schemas.android.com/apk/res-auto"
              android:orientation="horizontal"
              android:layout_width="wrap_content"
                android:background="@drawable/tablerowdrawable"
              android:layout_height="wrap_content">
             <TextView
                    android:layout_width="80dp"
                    xmlns:android="http://schemas.android.com/apk/res/android"
                    android:layout_height="wrap_content"
                    android:textColor="@color/black"
                    android:textSize="@dimen/space15"
                    android:textAlignment="center"
                    android:id="@+id/tv_date_vt"
                    android:minLines="3"
                    android:background="@drawable/tablerowdrawable"
                    android:maxLines="3"
            />
             <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tablerowdrawable"
                    android:textColor="@color/black"
                    android:textSize="@dimen/space15"
                    android:textAlignment="center"
                    android:minLines="3"
                    android:id="@+id/tv_dh"
                    android:maxLines="3"
            />
            <TextView
                    android:layout_width="100dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tablerowdrawable"
                    android:textColor="@color/black"
                    android:textSize="@dimen/space15"
                    android:textAlignment="center"
                    android:id="@+id/tv_extraworkinghr"
                    android:minLines="3"
                    android:maxLines="3"
            />
            <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="150dp"
                    android:minLines="3"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tablerowdrawable"
                    android:textColor="@color/black"
                    android:textSize="@dimen/space15"
                    android:textAlignment="center"
                    android:maxLines="3"
                    android:id="@+id/tv_actvity"
                    android:ellipsize="end"
            />
            <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tablerowdrawable"
                    android:textColor="@color/black"
                    android:textSize="@dimen/space15"
                    android:textAlignment="center"
                    android:id="@+id/tv_client"
                    android:minLines="3"
                    android:maxLines="3"
            />
            <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tablerowdrawable"
                    android:textColor="@color/black"
                    android:textSize="@dimen/space15"
                    android:textAlignment="center"
                    android:id="@+id/tv_location"
                    android:minLines="3"
                    android:maxLines="3"
            />
            <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tablerowdrawable"
                    android:textColor="@color/black"
                    android:textSize="@dimen/space15"
                    android:textAlignment="center"
                    android:id="@+id/tv_oa"
                    android:minLines="3"
                    android:maxLines="3"
            />
            <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tablerowdrawable"
                    android:textColor="@color/black"
                    android:textSize="@dimen/space15"
                    android:textAlignment="center"
                    android:id="@+id/tv_travelcharge"
                    android:minLines="3"  android:maxLines="3"
            />
            <TextView
                    android:layout_width="80dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tablerowdrawable"
                    android:textColor="@color/black"
                    android:textSize="@dimen/space15"
                    android:textAlignment="center"
                    android:id="@+id/tv_manday"
                    android:minLines="3" android:maxLines="3"

            />
            <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:background="@drawable/tablerowdrawable"
                    android:textColor="@color/black"
                    android:textSize="@dimen/space15"
                    android:textAlignment="center"
                    android:id="@+id/tv_remark"
                    android:minLines="3"
                    android:maxLines="3"
                    android:ellipsize="end"
            />
   </TableRow>